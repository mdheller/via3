<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="content-language" content="en">
        <meta name="language" content="English">
        <style>

body {
    padding:20px;
}

table, td, th {
    border-collapse: collapse;
    border:1px solid #aaa;
    padding:4px 8px;
}

        </style>
    </head>
    <body>
        <table>
            <tr>
                <th>Domain name</th>
                <th>Count</th>
                <th>Original link</th>
                <th>Via link</th>
                <th>Via 3 link</th>
            </tr>

        {% for domain in domains %}
            <tr>
                <td>{{ domain.name }}</td>
                <td>{{ domain.count }}</td>
                <td><a href="{{ domain.url }}" target="_orig">Original</a></td>
                <td><a href="{{ legacy_url_for(domain.url) }}" target="_via">Via</a></td>
                <td>
                    <a href="{{ url_for(domain.url, "null") }}" target="_via3">Null</a>
                    <a href="{{ url_for(domain.url, "htmlparser") }}" target="_via3">HTMLParser</a>
                    <a href="{{ url_for(domain.url, "lxml") }}" target="_via3">LXML</a>
                </td>
            </tr>
        {% endfor %}
        </table>
    </body>
</html>

